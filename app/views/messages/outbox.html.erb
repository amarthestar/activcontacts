<%= render :partial => "layouts/first" %>
<%= image_tag("/images/u22.jpg")%>
<div align=right style="margin-top:140px">
<%= image_tag("/images/u104.jpg")%>
<%= link_to 'Inbox',inbox_path %>
<%= image_tag("/images/u105.jpg")%>
<%= link_to 'Sent Messages',outbox_path %>
</div>
<div align=center>
<% if @mesg.present? %>
<h2>Messages</h2>
<h3>Sent Messages</h3>
<h3></h3>
<table bgcolor border=5>
  <tr >
<th>To</th>
<th>Subject</th>
<th>Date sent</th>
<th>Action</th>
</tr>
<% i=0 %>
<% @mesg.each do |p| %>
<tr>
 <td><%=@rec[i]%><br></td>
 <td><%=link_to p.subject,"/msgout/#{p.id}" %><br></td>
 <td><%=p.created_at%><br></td>
 <td><%= link_to 'Delete', :id=>p.id, :action => "destroyout" %>
</tr>
     <% i=i+1 %>
<% end %>
</table>
<% end %>
</div>




